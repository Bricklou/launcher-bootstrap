{
  "build": {
    "beforeDevCommand": "pnpm start --port 1420",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/tauri-applauncher-bootstrap",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "launcher-bootstrap",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTools",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "ovh.bricklou.launcher-bootstrap",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "launcher-bootstrap",
        "width": 400,
        "height": 500
      }
    ]
  }
}